  "module": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/buffered-clone.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/buffered-clone/issues"
  },
  "homepage": "https://github.com/WebReflection/buffered-clone#readme",
